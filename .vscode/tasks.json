{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ build active file",
            "command": "/usr/bin/g++",
            "args": [
                "-g",
                "-o",
                "OpenSprinkler",
                "-DOSPI",
                "-DNO_GPIO",
                "main.cpp",
                "OpenSprinkler.cpp",
                "program.cpp",
                "opensprinkler_server.cpp",
                "weather.cpp",
                "utils.cpp",
                "gpio.cpp",
                "etherport.cpp",
                "mqtt.cpp",
                "-lpthread",
                "-lmosquitto"
            ],
            "options": {
                "cwd": "${workspaceRoot}/"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ]
}